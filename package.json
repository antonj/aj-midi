{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && yarn static-build && remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --minify",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "concurrently \"yarn static-dev\" \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve api/build",
    "static-build": "esbuild public/audio-worklet.ts --minify --bundle --outfile=public/audio-worklet.js '--define:process.env.NODE_ENV=\"\"'",
    "static-dev": "esbuild public/audio-worklet.ts --watch --bundle --outfile=public/audio-worklet.js '--define:process.env.NODE_ENV=\"development\"'"
  },
  "dependencies": {
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "@tonejs/midi": "^2.0.28",
    "esbuild": "^0.15.7",
    "lil-gui": "^0.16.1",
    "pitchfinder": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.13.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webmidi": "^2.0.6",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "jest": "^29.2.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
